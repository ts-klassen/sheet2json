{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Security-check Questions Export",
  "type": "object",
  "properties": {
    "cells": {
      "type": "object",
      "description": "Container for all mapped spreadsheet cells. The application unwraps this object so the properties below are presented as the mappable fields.",
      "properties": {
        "no": {
          "type": "object",
          "properties": {
            "cell": { "type": "string", "description": "A1-style reference" },
            "value": { "type": "string" }
          },
          "required": ["cell", "value"]
        },
        "titles": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "cell": { "type": "string" },
              "value": { "type": "string" }
            },
            "required": ["cell", "value"]
          }
        },
        "question": {
          "type": "object",
          "properties": {
            "cell": { "type": "string" },
            "value": { "type": "string" }
          },
          "required": ["cell", "value"]
        },
        "notes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "cell": { "type": "string" },
              "value": { "type": "string" }
            },
            "required": ["cell", "value"]
          }
        },
        "answer": {
          "type": "object",
          "properties": {
            "cell": { "type": "string" },
            "value": { "type": "string" }
          },
          "required": ["cell", "value"]
        },
        "answer_sup": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "cell": { "type": "string" },
              "value": { "type": "string" }
            },
            "required": ["cell", "value"]
          }
        }
      },
      "required": [
        "no",
        "titles",
        "question",
        "notes",
        "answer",
        "answer_sup"
      ]
    }
  },
  "required": ["cells"]
}
