{
  "name": "sheet-to-json-mapper",
  "version": "0.1.0",
  "description": "Front-end tool that maps Excel/CSV worksheets to JSON via visual UI.",
  "scripts": {
    "dev": "es-dev-server --app-index index.html --reload --port 8000",
    "build": "rollup -c",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "cypress": "cypress open",
    "cypress:run": "cypress run"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.5",
    "babel-jest": "^29.5.0",
    "cypress": "^12.15.0",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "rollup": "^3.26.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-commonjs": "^24.1.0",
    "es-dev-server": "^2.0.0",
    "serve": "^14.2.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
